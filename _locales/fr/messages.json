  "notification-sitecentrictagtriggered-title-ok": {
    "message": "TAG $TAGTYPE$ DÉCLENCHÉ",
    "description": "title of notification when the tag is correctly triggered",
    "placeholders": {
      "tagtype" : {
        "content" : "$1",
        "example" : "generic"
      }
    }
  },
  "notification-sitecentrictagtriggered-message-ok": {
    "message": "Tag $TAGTYPE$ déclenché en $FIRSTPARTY$ sur $WSPAGE$ seulement $TIMEELAPSED$ s après l'arrivée sur la page.\n\nDonnées récupérées par le site: $WSNAME$",
    "description": "content of notification when the tag is correctly triggered",
    "placeholders": {
      "tagtype" : {
        "content" : "$1",
        "example" : "generic"
      },
      "firstparty" : {
        "content" : "$2",
        "example" : "1st-party"
      },
      "wspage" : {
        "content" : "$3",
        "example" : "www.3suisses.fr"
      },
      "timeelapsed" : {
        "content" : "$4",
        "example" : "2.27"
      },
      "wsname" : {
        "content" : "$5",
        "example" : "3suisses-fr"
      }      
    }
  },

  "notification-sitecentrictagtriggered-title-false": {
    "message": "/!\\ PROBLEME AVEC LE TAG $TAGTYPE$ DÉCLENCHÉ",
    "description": "title of notification when the tag is not correctly triggered",
    "placeholders": {
      "tagtype" : {
        "content" : "$1",
        "example" : "generic"
      }
    }
  },
  "notification-sitecentrictagtriggered-message-false": {
    "message": "Aïe, le tag $TAGTYPE$ déclenché en $FIRSTPARTY$ sur $WSPAGE$ n'a pas été récupéré par notre système",
    "description": "content of notification when the tag is not correctly triggered",
    "placeholders": {
      "tagtype" : {
        "content" : "$1",
        "example" : "generic"
      },
      "firstparty" : {
        "content" : "$2",
        "example" : "3rd-party"
      },
      "wspage" : {
        "content" : "$3",
        "example" : "www.3suisses.fr"
      }
    }
  },
  "time-ago": {
    "message":"il y a $HOURS_AGO$h et $MINUTES_AGO$ min ($HOURS$h:$MIN$m:$SECS$s)",
